{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "open-in-trae",
  "title": "Open in Trae",
  "description": "Open current Finder directory, selected folder or file in Trae.",
  "icon": "icon.png",
  "author": "SilentDeep",
  "contributors": [
  ],
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Open in Trae",
      "description": "Open current Finder directory, selected folder or file in Trae.",
      "mode": "no-view"
    }
  ],
  "preferences": [
    {
      "name": "TraeVariant",
      "description": "Trae variant to open the selected finder into.",
      "type": "dropdown",
      "title": "Trae Variant",
      "required": false,
      "default": "Trae",
      "data": [
        {
          "title": "Trae",
          "value": "com.trae.app"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}